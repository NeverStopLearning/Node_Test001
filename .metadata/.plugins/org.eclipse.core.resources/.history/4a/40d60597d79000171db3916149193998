export default function test_redcr(state, action){
	
	
	switch(action.type){
		case "ADD_TODO":
			return Object.assign({},state, {
							                  todos: [
							                	  {
							                		  text:action.text,
							                		  completed:false,
							                		  id:getId(state)
							                	  },
							                	  ...state.todos
							                  ]
							                  
											});
		default:
			return state;
	}
	
}